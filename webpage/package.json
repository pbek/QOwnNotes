    "vuepress": "^1.8.0",
    "vuepress-plugin-canonical": "^1.0.0",
    "vuepress-plugin-feed": "^0.1.8",
    "vuepress-plugin-matomo": "^1.0.2",
    "vuepress-plugin-mermaidjs": "^1.8.0",
    "vuepress-plugin-seo": "^0.1.4",
